{
  "name": "gmu-cs-scheduler",
  "version": "1.0.0",
  "description": "",
  "main": "",
  "scripts": {
    "clean": "npx prettier \"src/**/*.{ts,tsx}\" --write --loglevel=\"warn\"",
    "lint": "tslint -c tslint.json 'src/**/*.{ts,tsx}'",
    "prebuild:ts-transpile": "rimraf ./bin/",
    "build:ts-transpile": "tsc",
    "prebuild:html": "rimraf ./dist/",
    "build:html": "webpack",
    "tslint-check": "tslint-config-prettier-check ./tslint.json",
    "check-code": "npm run clean && npm run lint",
    "pre-commit": "npm run check-code",
    "prestart": "npm run check-code",
    "start": "npm run build:html",
    "pretest": "npm run check-code",
    "test": "jest"
  },
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^23.3.9",
    "awesome-typescript-loader": "^5.2.1",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "npm": "^6.4.1",
    "prettier": "1.14.3",
    "rimraf": "^2.6.2",
    "source-map-loader": "^0.2.4",
    "ts-jest": "^23.10.4",
    "tslint": "^5.11.0",
    "tslint-config-prettier": "^1.15.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.4",
    "webpack": "^4.23.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "@types/react": "^16.4.18",
    "@types/react-dom": "^16.0.9",
    "react": "^16.6.0",
    "react-dom": "^16.6.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run pre-commit",
      "pre-push": "npm run pre-commit",
      "post-merge": "npm install"
    }
  },
  "prettier": {
    "tabWidth": 4,
    "bracketSpacing": false,
    "printWidth": 100
  }
}
